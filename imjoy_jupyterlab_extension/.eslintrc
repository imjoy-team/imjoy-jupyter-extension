{
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "class-methods-use-this": 0,
        "no-plusplus": 0,
        "no-minusminus": 0,
        "no-unused-vars": 0,
        "no-debugger": 0,
        "prettier/prettier": ["error"],
        "no-param-reassign": 0,
        "no-console": 0,
        "no-underscore-dangle": 0
    },
    "plugins": ["prettier"],
    "extends": ["airbnb", "prettier"]
}